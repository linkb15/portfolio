{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "whatsapp-clone-tauri",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/AppIcons/Assets.xcassets/AppIcon.appiconset/32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/Frame.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.whatsapp-clone.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 708,
        "resizable": false,
        "title": "whatsapp-clone-tauri",
        "width": 1120,
        "transparent": true,
        "decorations": false
      }
    ],
    "macOSPrivateApi": true
  }
}
